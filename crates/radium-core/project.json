{
	"name": "radium-core",
	"$schema": "../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "./radium-core/src",
	"targets": {
		"build": {
			"cache": true,
			"executor": "@monodon/rust:build",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/radium-core"
			},
			"configurations": {
				"production": {
					"release": true
				}
			}
		},
		"test": {
			"cache": true,
			"executor": "@monodon/rust:test",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/radium-core"
			},
			"configurations": {
				"production": {
					"release": true
				}
			}
		},
		"lint": {
			"cache": true,
			"executor": "@monodon/rust:lint",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/radium-core"
			}
		},
		"run": {
			"executor": "@monodon/rust:run",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/radium-core"
			},
			"configurations": {
				"production": {
					"release": true
				}
			}
		}
	},
	"tags": ["scope:backend", "type:app", "lang:rust"]
}
