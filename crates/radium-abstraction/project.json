{
	"name": "model-abstraction",
	"$schema": "../node_modules/nx/schemas/project-schema.json",
	"projectType": "library",
	"sourceRoot": "./model-abstraction/src",
	"targets": {
		"build": {
			"executor": "@monodon/rust:check",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/model-abstraction"
			}
		},
		"test": {
			"cache": true,
			"executor": "@monodon/rust:test",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/model-abstraction"
			},
			"configurations": {
				"production": {
					"release": true
				}
			}
		},
		"lint": {
			"cache": true,
			"executor": "@monodon/rust:lint",
			"outputs": ["{options.target-dir}"],
			"options": {
				"target-dir": "dist/target/model-abstraction"
			}
		}
	},
	"tags": ["scope:backend", "type:lib", "lang:rust"]
}
