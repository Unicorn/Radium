{
	"name": "ui",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/ui/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"command": "vite build",
				"cwd": "packages/ui"
			}
		},
		"type-check": {
			"executor": "nx:run-commands",
			"options": {
				"command": "tsc --noEmit",
				"cwd": "packages/ui"
			}
		},
		"test": {
			"executor": "nx:run-commands",
			"options": {
				"command": "vitest run",
				"cwd": "packages/ui"
			}
		}
	},
	"tags": ["type:lib", "scope:shared"],
	"implicitDependencies": ["state", "shared-types"]
}

